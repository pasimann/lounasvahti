{
  "name": "lounasvahti",
  "scripts": {
    "lint": "tslint --project tsconfig.json",
    "test": "jest",
    "build": "tsc",
    "watch": "tsc -w",
    "start": "nodemon target/index.js",
    "pretest": "npm run build && npm run lint"
  },
  "devDependencies": {
    "@types/axios": "0.14.0",
    "@types/cron": "1.2.1",
    "@types/dotenv": "4.0.2",
    "@types/jest": "21.1.6",
    "@types/moment": "2.13.0",
    "@types/node": "8.0.53",
    "@types/winston": "2.3.7",
    "jest": "21.2.1",
    "nodemon": "1.12.1",
    "ts-jest": "21.2.3",
    "tslint": "5.8.0",
    "tslint-config-standard": "7.0.0",
    "typescript": "2.6.1"
  },
  "dependencies": {
    "@slack/client": "3.14.2",
    "axios": "0.17.1",
    "cron": "1.3.0",
    "dotenv": "4.0.0",
    "module-alias": "2.0.2",
    "moment": "2.19.3",
    "source-map-support": "0.5.0",
    "winston": "2.4.0"
  },
  "jest": {
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "moduleFileExtensions": [
      "js",
      "ts"
    ],
    "moduleNameMapper": {
      "lounasvahti/(.*)": "<rootDir>/src/$1"
    },
    "testRegex": "(/test/.*|(\\.|/)test)\\.ts$"
  },
  "_moduleAliases": {
    "lounasvahti": "target"
  }
}
